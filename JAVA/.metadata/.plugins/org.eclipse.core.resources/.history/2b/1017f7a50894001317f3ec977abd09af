import java.util.Scanner;


public class EucildAlgorithm {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner scanny = new Scanner(System.in);
		long a =scanny.nextLong();
        long b =scanny.nextLong();
        
        while(b != 0) {
        	long rem = a % b ;
        	
        	a = b;
        	
        	b = rem;
        	
        }
        
        System.out.println(a);
	}

}
