package abstractDataTypes;

public class AVLTree {

	 private static class AVLNode<T>{
		 
		 AVLNode<T>        left;
		 AVLNode<T>        right;
		 T                 data;
		 int               height;
		 AVLNode(T data){
			 this(data,null,null);
			 
		 }
		 AVLNode(T data,AVLNode left,AVLNode right){
			 this.left   =     left;
			 this.right  =     right;
			 this.data   =     data;
			 this.height =     0;
		 }
	 }

}
