import abstractDataTypes.BoundedQueue;
import abstractDataTypes.BoundedStack;

 




 

public class Main {

	/**
	 * @param args
	 * @throws Exception 
	 */
	public static void main(String[] args) {
		
		
		BoundedStack bs = new BoundedStack();
		BoundedQueue bq = new BoundedQueue();
		String radar =  "radar" ;
		for(int i=0;i<radar.length();i++) {
		bs.push(radar.charAt(i));
		bq.add(radar.charAt(i));
		}
		StringBuffer sb = new StringBuffer();
		int size = bs.sizeOf();
		for(int i = 0 ; i < size ; i ++ ) {
			   
		String first = 	sb.append((char)bs.pop());
		}
		 
	    sb.setLength(0);
	    size = bq.getLength();
	    for(int i = 0 ; i < size ; i ++ ) {
			   
			sb.append((char)bq.remove());
		}
		 
	    
   
 
	 
		

	}

}
