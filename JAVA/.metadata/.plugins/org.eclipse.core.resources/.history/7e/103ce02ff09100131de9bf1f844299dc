import java.util.InputMismatchException;
import java.util.Scanner;


public class RecursionExample {

	public void start(){
      
     
System.out.println(this.digits(1001));
	 
		}
		 private int digits(int number){
			if(number < 10 ) {
				return 1;
			} else {
				return 1+ digits(number/10);
			}
		}

}
