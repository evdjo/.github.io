import java.util.Arrays;

import abstractDataTypes.Person;

 

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
   
	  Person a = new Person(27,"Gosho1",1500);
	  Person b = new Person(26,"Gosho",1500);
	  Person c = new Person(35,"Ivan",1350);
	  Person d = new Person(17,"Gosho1",1200);
	  Person e = new Person(55,"Gosho",1800);
	  Person f = new Person(55,"Ivan",1650);
	 Person[] array = new Person[6];
	 array[0] = a; 
	 array[1] = b; 
	 array[2] = c; 
	 array[3] = d; 
	 array[4] = e; 
	 array[5] = f; 
	Arrays.sort(array);
	 for( Person  x : array ){
		 System.out.println("Name is " +x.getName() +" Age is "+ x.getAge()+" Salary is " + x.getSalary());
	 }
	   
	}
}